{
  "name": "abaydullah/php-laravel-android-apk-parser",
  "type": "library",
  "description": "Read basic info about an application from android .apk file",
  "keywords": [
    "apk",
    "parser",
    "android",
    "laravel",
    "php"
  ],
  "homepage": "https://abaydullah.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Abaydullah",
      "email": "abaydullah786@gmail.com",
      "homepage": "https://abaydullah.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-simplexml": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-zip": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.1",
    "friendsofphp/php-cs-fixer": "^3.1"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/abaydullah/php-laravel-android-apk-parser.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "Abaydullah\\": "lib/"
    }
  },
  "scripts": {
    "tests": "@php phpunit --configuration=phpunit.xml",
    "cs": "PHP_CS_FIXER_IGNORE_ENV=true ./vendor/bin/php-cs-fixer fix ."
  },
  "minimum-stability": "dev"
}
